{
    "name": "arcanedev/seo-helper",
    "description": "SEO Helper is a package for Laravel 5 that provides tools and helpers for SEO.",
    "keywords": [
        "laravel", "seo", "google", "bing", "alexa", "yandex", "webmaster",
        "twitter", "opengraph", "og", "opengraph protocol", "helper"
    ],
    "authors": [
        {
            "name":  "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com",
            "role":  "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php":                 ">=5.5.9",
        "arcanedev/support":   "~3.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpcov":      "~2.0",
        "phpunit/phpunit":     "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\SeoHelper\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\SeoHelper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
